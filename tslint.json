{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "interface-name": [true, "never-prefix"],
    "camelcase": ["error"]
  },
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx", "**/*.vue"],
      "rules": {
        "camelcase": ["off"]
      }
    }
  ]
}